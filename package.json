{
  "author": {
    "email": "yucky.hito@essilor.com.sg",
    "name": "Yucky Hito"
  },
  "dependencies": {
    "@fluentui/react-components": "^9.40.0",
    "@fluentui/react-icons": "^2.0.222",
    "@reduxjs/toolkit": "^1.9.7",
    "@tauri-apps/api": "^1.5.1",
    "fp-ts": "^2.16.1",
    "fuse.js": "^7.0.0",
    "plotly.js-cartesian-dist": "^2.27.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-error-boundary": "^4.0.11",
    "react-helmet-async": "^1.3.0",
    "react-plotly.js": "^2.6.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.19.0",
    "scheduler": "0.20.2",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.0/xlsx-0.20.0.tgz",
    "xlsx-js-style": "^1.2.0"
  },
  "dependenciesMeta": {
    "@tauri-apps/cli@1.5.6": {
      "unplugged": true
    }
  },
  "description": "A data cleaning tool for Essilor",
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@tauri-apps/cli": "^1.5.6",
    "@types/eslint": "^8.44.7",
    "@types/node": "^20.9.1",
    "@types/plotly.js-cartesian-dist": "npm:@types/plotly.js@*",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/react-plotly.js": "^2.6.3",
    "@types/scheduler": "^0.16.6",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "@vitejs/plugin-basic-ssl": "^1.0.2",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "encoding": "^0.1.13",
    "eslint": "^8.53.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-fp-ts": "^0.3.2",
    "eslint-plugin-functional": "^6.0.0",
    "eslint-plugin-immutable": "^1.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsdoc": "^46.9.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-perfectionist": "^2.4.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-redux": "^4.1.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "lightningcss": "^1.22.1",
    "modern-normalize": "^2.0.0",
    "prettier": "^3.1.0",
    "supports-color": "^9.4.0",
    "typedoc": "^0.25.3",
    "typedoc-github-wiki-theme": "^1.1.0",
    "typedoc-plugin-frontmatter": "^0.0.2",
    "typedoc-plugin-markdown": "^4.0.0-next.27",
    "typedoc-plugin-merge-modules": "^5.1.0",
    "typedoc-plugin-missing-exports": "^2.1.0",
    "typedoc-theme-hierarchy": "^4.1.2",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vite-plugin-lightningcss": "^0.0.5",
    "vite-plugin-mkcert": "^1.16.0",
    "vite-plugin-pwa": "^0.16.7",
    "workbox-window": "^7.0.0"
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=8",
    "yarn": ">=4"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es2020": true
    },
    "extends": [
      "eslint:recommended",
      "airbnb",
      "airbnb-typescript",
      "airbnb/hooks",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/strict-type-checked",
      "plugin:@typescript-eslint/stylistic-type-checked",
      "plugin:functional/external-typescript-recommended",
      "plugin:functional/recommended",
      "plugin:functional/stylistic",
      "plugin:fp-ts/recommended",
      "plugin:fp-ts/recommended-requiring-type-checking",
      "plugin:immutable/recommended",
      "plugin:import/typescript",
      "plugin:json/recommended",
      "plugin:optimize-regex/all",
      "plugin:perfectionist/recommended-natural",
      "plugin:prettier/recommended",
      "plugin:react/recommended",
      "plugin:react/jsx-runtime",
      "plugin:react-redux/recommended",
      "plugin:react-hooks/recommended",
      "plugin:jsdoc/recommended-typescript-error"
    ],
    "ignorePatterns": [
      "dist",
      "dev-dist",
      "src-tauri",
      "html-docs"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": "latest",
      "project": [
        "./tsconfig.json",
        "./src/tsconfig.json"
      ],
      "sourceType": "module",
      "tsconfigRootDir": "."
    },
    "plugins": [
      "fp-ts",
      "functional",
      "immutable",
      "import",
      "jsdoc",
      "json-format",
      "optimize-regex",
      "perfectionist",
      "prettier",
      "react",
      "react-hooks",
      "react-redux",
      "react-refresh"
    ],
    "root": true,
    "rules": {
      "@typescript-eslint/consistent-type-exports": 2,
      "@typescript-eslint/consistent-type-imports": 2,
      "@typescript-eslint/semi": 0,
      "functional/no-expression-statements": 0,
      "functional/no-mixed-types": 0,
      "functional/no-return-void": 0,
      "functional/prefer-immutable-types": [
        1,
        {
          "enforcement": "ReadonlyShallow",
          "ignoreInferredTypes": true
        }
      ],
      "functional/functional-parameters": 0,
      "immutable/no-let": 2,
      "immutable/no-this": 2,
      "import/extensions": 0,
      "import/no-extraneous-dependencies": 0,
      "import/no-restricted-paths": 0,
      "import/no-self-import": 0,
      "import/no-unresolved": 0,
      "import/no-useless-path-segments": 0,
      "import/order": 0,
      "import/sort-imports": 0,
      "jsdoc/check-tag-names": [
        2,
        {
          "definedTags": [
            "category",
            "group",
            "remarks"
          ]
        }
      ],
      "jsdoc/informative-docs": 2,
      "jsdoc/require-asterisk-prefix": 2,
      "jsdoc/require-description": 2,
      "jsdoc/require-example": 2,
      "jsdoc/require-file-overview": 2,
      "jsdoc/require-hyphen-before-param-description": 2,
      "jsdoc/require-throws": 2,
      "react/jsx-props-no-spreading": 0,
      "redux/no-let": 0,
      "redux/no-mutation": 0,
      "redux/no-this": 0,
      "require-jsdoc": 2
    },
    "settings": {
      "import/parsers": {
        "@typescript-eslint/parser": [
          ".ts",
          ".tsx"
        ]
      },
      "import/resolver": {
        "typescript": {
          "alwaysTryTypes": true,
          "project": [
            "./tsconfig.json",
            "./src/tsconfig.json"
          ]
        }
      },
      "react": {
        "version": "detect"
      }
    }
  },
  "keywords": [
    "clean",
    "tool",
    "app",
    "essilor",
    "data"
  ],
  "license": "UNLICENSED",
  "name": "clean-tool-app",
  "overrides": {
    "rollup-plugin-terser": "@rollup/plugin-terser"
  },
  "packageManager": "yarn@4.0.0-rc.53",
  "prettier": {
    "bracketSameLine": true,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yuckyh/clean-tool-app.git"
  },
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite",
    "doc": "typedoc --options typedoc.json",
    "doc:all": "yarn doc && yarn doc:git",
    "doc:git": "typedoc --options git-typedoc.json",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "preview:clean": "tsc && vite build && vite preview"
  },
  "type": "module",
  "version": "1.0.0"
}
